  "defaultSettings": {
    "language": {
      "default": "ko",
      "supported": ["ko", "en", "ja", "zh"]
    },
    "layout": {
      "header": "fixed",
      "navigation": "horizontal",
      "footer": "minimal"
    },
    "features": {
      "darkMode": true,
      "animations": true,
      "lazyLoading": true,
      "seo": true,
      "analytics": true,
      "chatbot": false
    },
    "performance": {
      "imageOptimization": true,
      "codesplitting": true,
      "prefetching": true,
      "compression": true
    }
  },

  "imageGeneration": {
    "totalImages": 150,
    "categories": {
      "hero": { "count": 3, "priority": 1 },
      "products": { "count": 50, "priority": 1 },
      "gallery": { "count": 30, "priority": 2 },
      "lifestyle": { "count": 25, "priority": 2 },
      "about": { "count": 10, "priority": 3 },
      "blog": { "count": 15, "priority": 3 },
      "misc": { "count": 17, "priority": 3 }
    },
    "formats": ["SVG", "WebP", "JPEG"],
    "sources": ["generated", "unsplash", "custom"],
    "metadata": {
      "author": "Generated by {brandName}",
      "alt": "auto-generated",
      "watermark": false
    }
  },

  "seoSettings": {
    "sitemap": true,
    "robots": true,
    "structured": true,
    "openGraph": true,
    "twitter": true,
    "locale": "ko_KR"
  },

  "deploymentOptions": {
    "vercel": {
      "auto": true,
      "domain": "{brandName.toLowerCase()}.vercel.app"
    },
    "netlify": {
      "auto": false,
      "domain": "{brandName.toLowerCase()}.netlify.app"
    },
    "github": {
      "pages": true,
      "actions": true
    }
  },

  "customizationOptions": {
    "colors": {
      "editable": true,
      "presets": ["minimal", "bold", "pastel", "dark", "vintage"]
    },
    "fonts": {
