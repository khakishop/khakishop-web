      "secondary": ["Playfair Display", "Nanum Myeongjo"],
      "options": ["Roboto", "Open Sans", "Lato", "Montserrat"]
    },
    "layout": {
      "templates": ["default", "magazine", "portfolio", "ecommerce", "blog"],
      "customizable": true
    }
  },

  "integrations": {
    "cms": ["strapi", "contentful", "sanity"],
